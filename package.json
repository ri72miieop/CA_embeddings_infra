{
  "name": "ca_embed",
  "version": "1.0.0",
  "description": "embeddings serving solution for Community Archive",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "cli": "bun run src/cli/index.ts",
    "search": "bun run scripts/semantic-search.ts",
    "import-to-qdrant": "bun run scripts/import-to-qdrant.ts",
    "debug-qdrant": "bun run scripts/debug-qdrant-upload.ts",
    "analyze-failed-queue": "bun run scripts/analyze-failed-queue-items.ts --type failed",
    "analyze-completed-queue": "bun run scripts/analyze-failed-queue-items.ts --type completed",
    "analyze-queue": "bun run scripts/analyze-failed-queue-items.ts",
    "benchmark": "bun run scripts/benchmark-vector-store.ts",
    "download-images": "bun run --expose-gc scripts/download_images.ts",
    "update-metadata": "bun run scripts/update-metadata.ts"
  },
  "dependencies": {
    "@ai-sdk/deepinfra": "^1.0.20",
    "@ai-sdk/openai": "^2.0.53",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/lib-storage": "^3.700.0",
    "@dsnp/parquetjs": "^1.8.7",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/static": "^7.0.4",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.1",
    "@opentelemetry/instrumentation-fastify": "^0.38.0",
    "@opentelemetry/instrumentation-http": "^0.52.1",
    "@opentelemetry/sdk-node": "^0.52.1",
    "@opentelemetry/semantic-conventions": "^1.25.1",
    "@qdrant/js-client-rest": "^1.15.1",
    "@qdrant/qdrant-js": "^1.15.1",
    "@supabase/supabase-js": "^2.79.0",
    "ai": "^5.0.54",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "dotenv": "^16.3.1",
    "duckdb": "^1.4.3",
    "duckdb-async": "^1.4.2",
    "fastify": "^4.24.3",
    "hyparquet": "^1.20.1",
    "hyparquet-compressors": "^1.1.1",
    "hyparquet-writer": "^0.9.0",
    "json-bigint": "^1.0.0",
    "ora": "^9.0.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "prom-client": "^15.1.0",
    "ssh2": "^1.17.0",
    "undici": "^7.16.0",
    "uuid": "^9.0.1",
    "voyageai": "^0.0.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.8.10",
    "@types/ssh2": "^1.15.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "keywords": [
    "embeddings",
    "vector-database",
    "qdrant",
    "typescript",
    "bun",
    "observability"
  ],
  "author": "",
  "license": "MIT"
}