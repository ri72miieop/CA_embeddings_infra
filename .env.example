# Server Configuration
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# Observability
ENABLE_METRICS=true
METRICS_PORT=9090
ENABLE_TRACING=true
JAEGER_ENDPOINT=http://localhost:14268/api/traces
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000

# Security
CORS_ORIGIN=*
HELMET_ENABLED=true

# Embedding Generation Configuration
EMBEDDING_GENERATION_ENABLED=true
EMBEDDING_PROVIDER=deepinfra
EMBEDDING_MODEL=Qwen/Qwen3-Embedding-4B
EMBEDDING_API_KEY=your_deepinfra_api_key_here
EMBEDDING_TIMEOUT=30000
EMBEDDING_RETRIES=2

# Embedding Call Storage
EMBEDDING_STORAGE_ENABLED=true
EMBEDDING_STORAGE_PATH=./data/embedding-calls

#To create embeddings as data is inserted on supabase
SUPABASE_URL=
SUPABASE_ANON_KEY=


##BENCHMARK - only necessary if you want to benchmark several machines, certificate must be in the OpenSSH format

SSH_USERNAME=
SSH_CERT_PATH=